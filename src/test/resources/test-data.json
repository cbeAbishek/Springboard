        "HTML": ["test-summary", "charts", "detailed-logs", "screenshots"],
        "CSV": ["test-name", "status", "duration", "error-message"],
        "XML": ["testcase", "failure", "error", "skipped"]
      }
    },
    "databaseValidation": {
      "requiredTables": [
        "test_batches",
        "test_cases",
        "test_executions",
        "test_schedules",
        "test_reports",
        "test_screenshots",
        "test_logs"
      ],
      "requiredIndexes": [
        "idx_batch_id",
        "idx_status",
        "idx_created_at",
        "idx_test_case_id",
        "idx_execution_id"
      ]
    }
  },
  "performanceBaselines": {
