{
  "blazeDemo": {
    "testScenarios": [
      {
        "scenarioName": "ValidFlightBooking",
        "testData": {
          "fromCity": "Boston",
          "toCity": "London",
          "passenger": {
            "name": "John Doe",
            "address": "123 Test Street",
            "city": "Test City",
            "state": "Test State",
            "zipCode": "12345"
          },
          "payment": {
            "cardType": "Visa",
            "cardNumber": "4111111111111111",
            "expiryMonth": "12",
            "expiryYear": "2025",
            "nameOnCard": "John Doe"
          }
        },
        "expectedResults": {
          "bookingSuccess": true,
          "confirmationPageTitle": "Thank you for your purchase today!",
          "requiredElements": ["confirmation-table", "booking-details"]
